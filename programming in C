 printf("ounze:%f\nmiligram:%lf",m1,m1*28349.52);
 else if(strcmp(n2,"microgram")==0)
 printf("ounze:%f\nmicrgram:%lf",m1,m1*28349523.1);
 else if(strcmp(n2,"quintal")==0)
 printf("ounze:%f\nquintal:%lf",m1,m1*0.00028349);
 else if(strcmp(n2,"pound")==0)
 printf("ounze:%f\npound:%lf",m1,m1*0.0625);
 else if(strcmp(n2,"carat")==0)
 printf("ounze:%f\ncarat:%lf",m1,m1*141.7476);
 else
 printf("enter the unit corrrectly");
}
 else if(strcmp(n1,"carat")==0){
 if(strcmp(n2,"tonne")==0)
 printf("carat:%f\ntonne:%lf",m1,m1*0.0000002);
 else if(strcmp(n2,"kilogram")==0)
 printf("carat:%f\nkilogram:%lf",m1,m1*0.0002);
 else if(strcmp(n2,"gram")==0)
 printf("carat:%f\ngram:%lf",m1,m1*0.2);
 else if(strcmp(n2,"milligram")==0)
 printf("carat:%f\nmilligram:%lf",m1,m1*200.0);
 else if(strcmp(n2,"microgram")==0)
 printf("carat:%f\nmicrogram:%lf",m1,m1*200000.0);
 else if(strcmp(n2,"quintal")==0)
 printf("carat:%f\nquintal:%lf",m1,m1*0.000002);
 else if(strcmp(n2,"pound")==0)
 printf("carat:%f\npound:%lf",m1,m1*0.00044092);
 else if(strcmp(n2,"ounze")==0)
 printf("carat:%f\nounze:%lf",m1,m1*0.00705479);
 else
 printf("enter the unit corrrectly");
 }
 else
 printf("enter the unit corrrectly");
}
 //function for bmi calculator
 void bmi(){
 float weight, height;
 printf("BMI CALCULATOR\n");
 printf("---------------\n");
 printf("Enter weight in kilograms: ");
 scanf("%f", &weight);
 printf("Enter height in centimeters: ");
 scanf("%f", &height);
 height = height / 100.0;
 float bmi = weight / (height * height);
 printf("BMI: %.2f\n", bmi);
 }
 void time(){
 printf("TIME CONVERTOR\n");
 printf("---------------\n");
 float m1;
 char n1[50],n2[50];
 printf("1.hour\n");
 printf("2.minute\n");
 printf("3.second\n");
 printf("4.millisecond\n");
printf("5.microsecond\n");
 printf("enter the input unit:");
 scanf("%s",&n1);
 strlwr(n1);
 printf("enter the input value:");
 scanf("%f",&m1);
 printf("enter the unit to be converted:");
 scanf("%s",&n2);
 strlwr(n2);
 if(strcmp(n1,n2)==0){
 printf("both the units are same");}
 else if(strcmp(n1,"hour")==0){
 if(strcmp(n2,"minute")==0)
 printf("hour:%f\nminute:%lf",m1,m1*60.0);
 else if(strcmp(n2,"second")==0)
 printf("hour:%f\nsecond:%lf",m1,m1*3600.0);
 else if(strcmp(n2,"millisecond")==0)
 printf("hour:%f\nmillisecond:%lf",m1,m1*3600000.0);
 else if(strcmp(n2,"microsecond")==0)
 printf("hour:%f\nmicrosecond:%lf",m1,m1*3600000000.0);
 else
 printf("enter the unit corrrectly");
 }
else if(strcmp(n1,"minute")==0){
 if(strcmp(n2,"hour")==0)
 printf("minute:%f\nhour:%lf",m1,m1*0.0167);
 else if(strcmp(n2,"second")==0)
 printf("minute:%f\nsecond:%lf",m1,m1*60.0);
 else if(strcmp(n2,"millisecond")==0)
 printf("minute:%f\nmillisecond:%lf",m1,m1*60000.0);
 else if(strcmp(n2,"microsecond")==0)
 printf("minute:%f\nmicrosecond:%lf",m1,m1*60000000000.0);
 else
 printf("enter the unit corrrectly");
 }
 else if(strcmp(n1,"second")==0){
 if(strcmp(n2,"hour")==0)
 printf("second:%f\nhour:%lf",m1,m1*0.0003);
 else if(strcmp(n2,"minute")==0)
 printf("second:%f\nminute:%lf",m1,m1*0.0167);
 else if(strcmp(n2,"millisecond")==0)
 printf("second:%f\nmillisecond:%lf",m1,m1*0.001);
 else if(strcmp(n2,"microsecond")==0)
 printf("second:%f\nmicrosecond:%lf",m1,m1*1000.0);
 else
 printf("enter the unit corrrectly");
}
 else if(strcmp(n1,"millisecond")==0){
 if(strcmp(n2,"hour")==0)
 printf("millisecond:%f\nhour:%lf",m1,m1*0.0000002778);
 else if(strcmp(n2,"minute")==0)
 printf("millisecond:%f\nminute:%lf",m1,m1*0.0000167);
 else if(strcmp(n2,"second")==0)
 printf("millisecond:%f\nsecond:%lf",m1,m1*0.001);
 else if(strcmp(n2,"microsecond")==0)
 printf("millisecond:%f\nmicrosecond:%lf",m1,m1*1000.0);
 else
 printf("enter the unit corrrectly");
 }
 else if(strcmp(n1,"microsecond")==0){
 if(strcmp(n2,"hour")==0)
 printf("microsecond:%f\nhour:%lf",m1,m1*0.0000000002778);
 else if(strcmp(n2,"minute")==0)
 printf("microsecond:%f\nminute:%lf",m1,m1*0.0000000167);
 else if(strcmp(n2,"second")==0)
 printf("microsecond:%f\nhour:%lf",m1,m1*0.000001);
 else if(strcmp(n2,"millisecond")==0)
 printf("microsecond:%f\nhour:%lf",m1,m1*0.001);
 else
printf("enter the unit corrrectly");
 }
 else
 }
 int main(){
 int n;
 printf("enter the unit corrrectly");
 printf("WELCOME TO CONVERTOR\n");
 printf("---------------\n");
 printf("1.LENGTH CONVERTOR\n");
 printf("2.TMPERATURE CONVERTOR\n");
 printf("3.MASS CONVERTOR\n");
 printf("4.TIME CONVERTOR\n");
 printf("5.BMI CALCULATOR\n");
 printf("---------------\n");
 printf("enter the number to open the particular convertor:");
 scanf("%d",&n);
 printf("---------------\n");
 switch(n){
 case(1):
 length();
 break;
 case(2):
temp();
 break;
 case(3):
 mass();
 break;
 case(4):
 time();
 break;
 case(5):
 bmi();
 break;
 default:
 printf("the number is invalid");
 }
 return 0;
 }
